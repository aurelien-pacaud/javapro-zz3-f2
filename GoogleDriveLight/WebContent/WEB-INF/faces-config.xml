<faces-config xmlns="http://java.sun.com/xml/ns/javaee"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"	version="2.0">		<!-- Déclaration des managed beans -->	<managed-bean>		<managed-bean-name>fileTreeBean</managed-bean-name>		<managed-bean-class>org.isima.ui.faces.bean.view.FileTreeBean</managed-bean-class>		<managed-bean-scope>session</managed-bean-scope>	</managed-bean>	<managed-bean>		<managed-bean-name>loginManagedBean</managed-bean-name>		<managed-bean-class>org.isima.ui.faces.bean.login.LoginManagedBean</managed-bean-class>		<managed-bean-scope>session</managed-bean-scope>	</managed-bean>	<managed-bean>		<managed-bean-name>fileListBean</managed-bean-name>		<managed-bean-class>org.isima.ui.faces.bean.view.FileListBean</managed-bean-class>		<managed-bean-scope>session</managed-bean-scope>	</managed-bean>		<!-- Règles de navigation -->	<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginSuccess</from-outcome>			<to-view-id>/app/main.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginFailed</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>		</navigation-case>	</navigation-rule>	<navigation-rule>		<display-name>app/main.xhtml</display-name>		<from-view-id>/app/main.xhtml</from-view-id>		<navigation-case>			<from-outcome>logoutSuccess</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>	<resource-bundle> 		<base-name>org.isima.resources.lang.messages</base-name> 		<var>msg</var> 	</resource-bundle>	
</faces-config>